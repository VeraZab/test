  "description": "New and improved branding and marketing pages with React and next.js.",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "export": "next build && next export",
    "ghpages": "cp .nojekyll out && git add out; timestamp=$(date \"+%c\") && git commit -m \"$timestamp deploy to gh-pages\" && git subtree push --prefix out origin gh-pages",
