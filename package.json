{
  "name": "plotly-next",
  "version": "0.0.1",
  "description": "New and improved branding and marketing pages with React and next.js.",
  "scripts": {
    "dev": "node server.js",
    "build": "next build",
    "export": "next export",
    "ghpages": "cp .nojekyll out && git add out; timestamp=$(date \"+%c\") && git commit -m \"$timestamp deploy to gh-pages\" && git subtree push --prefix out origin gh-pages",
    "deploy": "yarn run export && yarn run ghpages",
    "start": "NODE_ENV=production node server.js",
    "heroku-postinstall": "yarn run build",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write './{components,pages}/**/*.js'",
    "precommit": "yarn format"
  },
  "engines": {
    "node": "7.x.x"
  },
  "author": "Thomas Osmonson <hello@ineffable.co>",
  "license": "",
  "dependencies": {
    "antd": "^2.8.1",
    "autoprefixer": "6.7.6",
    "axios": "^0.16.2",
    "babel-plugin-module-resolver": "2.5.0",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-plugin-wrap-in-js": "^1.1.0",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "chromatic-sass": "^0.1.8",
    "express": "^4.15.2",
    "glob": "7.1.1",
    "husky": "^0.14.3",
    "in-view": "^0.6.1",
    "include-media": "^1.4.9",
    "isomorphic-fetch": "^2.2.1",
    "lozad": "^1.0.6",
    "lru-cache": "^4.1.1",
    "mdi": "^1.8.36",
    "module-alias": "^2.0.1",
    "next": "^4.1.0",
    "next-routes": "^1.0.27",
    "next-url-prettifier": "^1.1.2",
    "node-sass": "^4.4.0",
    "normalize-scss": "^6.0.0",
    "nprogress": "^0.2.0",
    "postcss-easy-import": "2.0.0",
    "postcss-loader": "1.3.3",
    "postcss-uncss": "^0.14.0",
    "prettier": "^1.7.4",
    "prismic-dom": "^2.0.4",
    "prismic-javascript": "^1.4.0",
    "proptypes": "^0.14.4",
    "raw-loader": "^0.5.1",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-ga": "^2.2.0",
    "react-sticky": "5.0.8",
    "react-swipe": "^5.0.6",
    "react-syntax-highlighter": "^5.0.0",
    "sass-loader": "^6.0.5",
    "swipe-js-iso": "^2.0.3",
    "vargs": "^0.1.0",
    "webpack": "2"
  },
  "devDependencies": {
    "babel-preset-env": "^1.2.1",
    "react-scripts": "^1.0.10"
  },
  "now": {
    "alias": "plotly-prismic.ineffable.digital"
  }
}
