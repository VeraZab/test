{
  "name": "plotly-next",
  "version": "0.0.2",
  "description": "New and improved branding and marketing pages with React and next.js.",
  "scripts": {
    "dev": "node server.js",
    "build": "next build",
    "preexport": "yarn run build",
    "export": "next export",
    "postexport": "cp .next/service-worker.js out",
    "ghpages": "cp .nojekyll out && git add out; timestamp=$(date \"+%c\") && git commit -m \"$timestamp deploy to gh-pages\" && git subtree push --prefix out origin gh-pages",
    "deploy": "yarn run export && yarn run ghpages",
    "start": "NODE_ENV=production node server.js",
    "heroku-postinstall": "yarn run build",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write './{components,pages}/**/*.js'"
  },
  "engines": {
    "node": "9.x.x"
  },
  "author": "Thomas Osmonson <hello@ineffable.co>",
  "license": "",
  "dependencies": {
    "axios": "^0.17.0",
    "babel-plugin-module-resolver": "2.5.0",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-plugin-wrap-in-js": "^1.1.0",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.5.0",
    "express": "^4.15.2",
    "fast-sass-loader": "^1.4.0",
    "fs": "^0.0.1-security",
    "glob": "7.1.1",
    "include-media": "^1.4.9",
    "lozad": "^1.0.9",
    "lru-cache": "^4.1.1",
    "mdi": "^1.8.36",
    "module-alias": "^2.0.1",
    "next": "^4.1.3",
    "next-redux-wrapper": "^1.3.4",
    "next-routes": "^1.0.27",
    "node-sass": "^4.4.0",
    "normalize-scss": "^6.0.0",
    "nprogress": "^0.2.0",
    "parse-github-url": "^1.0.0",
    "postcss-clean": "^1.1.0",
    "postcss-cssnext": "^3.0.2",
    "postcss-easy-import": "2.0.0",
    "postcss-loader": "^2.0.8",
    "postcss-nested": "^2.1.2",
    "postcss-scss": "^1.0.2",
    "postcss-uncss": "^0.14.0",
    "prismic-dom": "^2.0.4",
    "prismic-javascript": "^1.4.0",
    "proptypes": "^0.14.4",
    "raw-loader": "^0.5.1",
    "react": "^16.0.0",
    "react-codemirror": "^1.0.0",
    "react-codemirror2": "^3.0.7",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.0.0",
    "react-ga": "^2.2.0",
    "react-plotly.js": "^1.0.3",
    "react-redux": "^5.0.6",
    "react-syntax-highlighter": "^5.8.0",
    "redux": "^3.7.2",
    "shortid": "^2.2.8",
    "sw-precache-webpack-plugin": "^0.11.4",
    "webpack": "2"
  },
  "devDependencies": {
    "babel-preset-env": "^1.2.1",
    "now": "^9.0.0-canary.8",
    "prettier": "^1.7.4",
    "react-scripts": "^1.0.10",
    "redux-devtools-extension": "^2.13.2"
  },
  "now": {
    "alias": "plotly-prismic.ineffable.digital"
  }
}
