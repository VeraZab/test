  "scripts": {
    "build": "next build",
    "deploy": "yarn run export && yarn run ghpages",
    "dev": "node server.js",
    "export": "next export",
    "prettier": "prettier --write './{components,pages}/**/*.js'",
    "test:prettier": "prettier -l './{components,pages}/**/*.js' && echo -e '\\033[0;32m'PASS'\\033[0m'",
    "ghpages": "cp .nojekyll out && git add out; timestamp=$(date \"+%c\") && git commit -m \"$timestamp deploy to gh-pages\" && git subtree push --prefix out origin gh-pages",
    "heroku-postinstall": "yarn run build",
    "postexport": "cp .next/service-worker.js out/_next",
    "preexport": "yarn run build",
    "start": "NODE_ENV=production node server.js",
    "now-build": "next build && next export -o dist"
  }
