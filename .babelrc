{
  "presets": [
    "next/babel"
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": [
          "./"
        ],
        "alias": {
          "components": "./components",
          "static": "./static",
          "config": "./config",
          "store": "./store",
          "lib": "./lib",
          "styles": "./styles"
        }
      }
    ],
    [
      "wrap-in-js",
      {
        "extensions": [
          "css$",
          "scss$"
        ]
      }
    ],
    [
      "styled-components",
      {
        "ssr": true,
        "displayName": false,
        "preprocess": false
      }
    ]
  ],
  "ignore": []
}
